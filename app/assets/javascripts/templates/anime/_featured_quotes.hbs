{{#each featuredQuotes}}
  <div class="rounded-block col-sm-12 clearfix">
    <p><strong>{{characterName}}:</strong> {{content}}</p>
    <span class="favorited-by">Favorited by {{pluralize favoriteCount "user" "users"}} - <a href="#">Favorite this quote</a></span>
    <span class="report-quote"><a href="#">Report Quote</a></span>
  </div>
{{/each}}
<a {{bind-attr href=fullQuotesURL}} class="btn btn-default">view all</a>
